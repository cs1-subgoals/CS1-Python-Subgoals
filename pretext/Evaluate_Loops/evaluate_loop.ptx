    <subsection xml:id="subsec-nstdlps">
        <title>Nested Loops</title>
        <program language="python"><input>
i = 0
j = 0
m = 8
n = 5
while i &lt; n:
    j = 0
    while j &lt; m:
        print("*", end="")
        j += 1
    print()
    i += 1
</input></program>
<program language="python"><input>
for m in range(0, 4):
    for n in range(5, 0, -1):
        print("#", end="")
    print()
</input></program>
<program language="python"><input>
for m in range(0, 4):
    for n in range(0, m+1):
        print("$", end="")
    print()
</input></program>
<program language="python"><input>
for m in range(1, 5):
    for n in range(1, m+1):
        print(" ", end="")
    print("$")
</input></program>        
