    <subsection xml:id="subsec-scafmmf">
        <title>Sum, Count, Accumulate, Filter, Min/Max, Find</title>
<program language="python"><input>
high_numbers = [300, 200, 100, 400, 500, 300]
low_numbers = [1, 3, 2, 4, 5, 3]
mixed_numbers = [5, 3, -4, 7, 2]
</input></program>
<program language="python"><input>
total = 0
for number in low_numbers:
    total = total + number
print(total)
</input></program>
<program language="python"><input>
total = 0
for number in high_numbers:
    total = total + number // 100
print(total)
</input></program>
<program language="python"><input>
total = 0
for number in high_numbers:
    total = total + 100
print(total)
</input></program>
<program language="python"><input>
combined = ""
for number in low_numbers:
    combined = combined + str(number)
print(combined)
</input></program>
<program language="python"><input>
all_high = True
for number in high_numbers:
    all_high = all_high and number > 100
print("Are the numbers all greater than 100?", all_high)
</input></program>
<program language="python"><input>
total = 0
for number in low_numbers:
    if number % 2 == 0:
        total += number
print(total)
</input></program>
<program language="python"><input>
total = 0
for number in high_numbers:
    if number &lt;= 200:
        total += number
print(total)
</input></program>
<program language="python"><input>
target = None
for number in mixed_numbers:
    if number &lt; 0:
        target = number
print(target)
</input></program>
<program language="python"><input>
highest = mixed_numbers[0]
for number in mixed_numbers:
    if number > highest:
        highest = number
print(highest)
</input></program>
