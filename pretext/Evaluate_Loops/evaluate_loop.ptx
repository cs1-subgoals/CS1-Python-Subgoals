    <subsection xml:id="subsec-wlcntr">
        <title>While Loops Counter</title>
        <program language="python"><input>
counter = 0
total = 0
while counter &lt; 50:
    if counter % 5 == 0:
        total += counter
    counter += 1
print(total)
</input></program>
        <program language="python"><input>
i = 0
while i &lt; 3:
    print("hi")
    i += 1
</input></program>
        <program language="python"><input>
i = 10
while i > 1:
    print(i, end=" ")
    i -= 1
</input></program>
<program language="python"><input>
i = 10
while i > 1:
    print(i, end=" ")
    i += 1
</input></program>
<program language="python"><input>
i = 0
total = 0
while i &lt;= 50:
    total += i
    i += 5
print(i)
</input></program>
<program language="python"><input>
counter = 0
