    <transition />
    <subsection>
        <title>Evaluate Expressions-WE2-P2</title>

        <p>
            <term>Given</term>
        </p>


        <program language="python">
            <input>
alpha = 2
beta = 1
delta = '3.0'
theta = -1.3
            </input>
        </program>

        <exercises>
        <exercise>
            <statement>
                <p>
                    Q15: What is the value of gamma?
                    <program language="python">
                        <input>gamma = beta + alpha </input>
                    </program>
                    <var/>
                </p>
            </statement>
            <setup> <var> <condition number="3">
            <feedback>
                <p>
                    Correct
                </p>
            </feedback>
            </condition> <condition string='.*'>

            <feedback>
                <p>
                    Incorrect
                </p>
            </feedback>
            </condition> </var> </setup>
        </exercise>

        <exercise>
            <statement>
                <p>
                    Q16: What is the value of rho?
                    <program language="python">
                        <input>rho = alpha + theta</input>
                    </program>
                    <var/>
                </p>
            </statement>
            <setup> <var> <condition number="0.7" tolerance=".001">
            <feedback>
                <p>
                    Correct
                </p>
            </feedback>
            </condition> <condition string='.*'>

            <feedback>
                <p>
                    Incorrect
                </p>
            </feedback>
            </condition> </var> </setup>
        </exercise>

        <exercise xml:id="m-Expr-WE2-P2-16">
            <statement>
                <p>
                    Q17:
                    <program language="python">
                        <input>alpha = delta</input>
                    </program>
                </p>

                <p>
                    What type of value is stored in alpha?
                </p>
            </statement>


            <choices>
                <choice>
                    <statement>
                        <p>
                            Number (integer)
                        </p>
                    </statement>

                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice correct="yes">
                    <statement>
                        <p>
                            String
                        </p>
                    </statement>

                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>
                            Number (double)
                        </p>
                    </statement>

                    <feedback>
                        <p/>
                    </feedback>
                </choice>

                <choice>
                    <statement>
                        <p>
                            This is an invalid operation
                        </p>
                    </statement>

                    <feedback>
                        <p/>
                    </feedback>
                </choice>
            </choices>
        </exercise>
        </exercises>
