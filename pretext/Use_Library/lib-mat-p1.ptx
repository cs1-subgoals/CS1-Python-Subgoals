            <exercises>
                <exercise xml:id="m-Lib-WE1-P1-1"> <!-- insert WE number -->
                    <statement><p>Q1: What keyword is used to access a library?</p></statement>
                    <choices>
                        <choice correct="yes">
                            <statement><p>import</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>access</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>use</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>None of these</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                    </choices>
                </exercise>
                <exercise xml:id="m-Lib-WE1-P1-2"> <!-- insert WE number -->
                    <statement><p>Q2: What keyword is used to alias a library?</p></statement>
