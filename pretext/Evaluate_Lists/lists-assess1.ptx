    <exercise xml:id="assess-arrays-1-1">
        <statement>
            <p>
                Q1: Assuming that the following declaration has been made, which of the following code segments correctly interchanges the value of <c>arr[0]</c> and <c>arr[5]</c>?
            </p>


            <program language="python">
                <input>
arr = [5, 12, -22, 6, 11, 0, 26, 42, 99, 75];
# ----
# A
arr[0] = 5
arr[5] = 0
# ----
# B
arr[0] = arr[5]
arr[5] = arr[0]
# ----
# C
k = arr[5]
arr[0] = arr[5]
arr[5] = k
# ----
# D
k = arr[0]
arr[0] = arr[5]
arr[5] = k
# ----
# E
k = arr[5]
arr[5] = arr[0]
arr[0] = arr[5]
                </input>
            </program>
        </statement>


        <choices>
            <choice>
                <statement>
                    <p>
                        See answer A above
                    </p>
                </statement>

                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>
                        See answer B above
                    </p>
                </statement>

                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>
                        See answer C above
                    </p>
                </statement>

                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice correct="yes">
                <statement>
                    <p>
                        See answer D above
                    </p>
                </statement>

                <feedback>
                    <p/>
                </feedback>
            </choice>

            <choice>
                <statement>
                    <p>
                        See answer E above
                    </p>
                </statement>

                <feedback>
                    <p/>
                </feedback>
            </choice>
        </choices>
    </exercise>
