    <exercises>
        <exercise xml:id="m-Func-WE1-P1-1">
            <statement>
                <p>Q1: What is the output of the following code?</p>
                <program language="python"><input>
                    def foo(a, b):
                        if a &gt; b:
                            return "a is larger!"
                        elif b &gt; a:
                            return "b is larger!"
                        else:
                            return "a and b are equal!"

                    a = 10
                    b = 15
                    result = foo(b, a)
                    print(result)
                </input></program>
            </statement>
            <choices>
                <choice correct="yes">
                    <statement><p>"a is larger!"</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>"b is larger!"</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>"a and b are equal!"</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>Compiler error</p></statement>
                    <feedback><p/></feedback>
                </choice>
            </choices>
        </exercise>
