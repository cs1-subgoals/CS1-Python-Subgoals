        <exercise xml:id="m-Func-WE1-P1-6">
            <statement>
                <p>Q6: What is the output of the following code?</p>
                <program language="python"><input>
                    a = 10
                    c = 3

                    def foo(a, b):
                        b += a
                        return bar(b, c)
                    def bar(alpha, beta):
                        return alpha // beta

                    result = foo(2, 4)
                    print(result)
                </input></program>
            </statement>
            <choices>
                <choice>
                    <statement><p>2</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>4</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>4.3</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice correct="yes">
                    <statement><p>Compiler error</p></statement>
                    <feedback><p/></feedback>
                </choice>
            </choices>
        </exercise>
        <exercise xml:id="m-Func-WE1-P1-7">
            <statement>
                <p>Q7: What is the output of the following code?</p>
                <program language="python"><input>
                    def alpha(var1):
                        return var1 - 5

                    def beta(var1):
                        return var1 * var1

                    my_var = 3
                    result = alpha(beta(my_var))
                    print(result)
                </input></program>
            </statement>
            <choices>
                <choice correct="yes">
                    <statement><p>4</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>-2</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>-4</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>Compiler error</p></statement>
                    <feedback><p/></feedback>
                </choice>
            </choices>
        </exercise>
