            <exercises>
                <exercise xml:id="m-Expr-WE10_-P1-67">
                    <statement><p>Q67: What is the value stored in result?</p>
                        <program language="python"><input>result = alpha and beta</input></program></statement>
                    <choices>
                        <choice correct="yes">
                            <statement><p>0</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>10</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>False</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>True</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                    </choices>
                </exercise>
