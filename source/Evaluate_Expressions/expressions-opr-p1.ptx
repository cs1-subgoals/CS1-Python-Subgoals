        <transition/>
        <subsection>
            <title>Expressions-WE4-P1</title>
            <p><term>Given</term></p>
            <program language="python"><input>
alpha = 2
beta = 1 
delta = 3 
omega = 2.5 
theta = -1.3 
kappa = 3.0 
            </input></program>
            <exercises>
                <exercise xml:id="m-Expr-WE4-P1-25">
                    <statement><p>Q25: lambda = alpha / kappa + delta</p></statement>
                    <choices>
                        <choice correct="yes">
                            <statement><p>valid</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice>
                            <statement><p>invalid</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                    </choices>
                </exercise>
                <exercise xml:id="m-Expr-WE4-P1-26">
                    <statement><p>Q26: eta = alpha * beta + (omega &#8211; theta) * kappa</p></statement>
                    <choices>
                        <choice>
                            <statement><p>valid</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                        <choice correct="yes">
                            <statement><p>invalid</p></statement>
                            <feedback><p/></feedback>
                        </choice>
                    </choices>
                </exercise>
            </exercises>
        </subsection>
        <program xml:id="ac-express-we4-p1" interactive="activecode" language="python">
            <input></input>
        </program>
