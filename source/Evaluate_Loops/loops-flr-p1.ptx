        <exercise xml:id="m-Loops-WE2-P1-1">
            <statement>
                <p>Q1: What is the value stored in total after the following code executes?</p>
                <program language="python"><input>
total = 0
for x in range(50, 0, -5):
    total += x
                </input></program>
            </statement>
            <choices>
                <choice>
                    <statement><p>50</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>0</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice correct="yes">
                    <statement><p>275</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>270</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>5</p></statement>
                    <feedback><p/></feedback>
                </choice>
            </choices>
        </exercise>
