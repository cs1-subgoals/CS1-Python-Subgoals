        <exercise xml:id="m-Loops-WE2-P1-6">
            <statement>
                <p>Q6: What is the output of the following loop?</p>
                <program language="python"><input>
sum = 0
for i in range(6):
    sum += i
print(sum)
                </input></program>
            </statement>
            <choices>
                <choice>
                    <statement><p>0</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>1</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice correct="yes">
                    <statement><p>15</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>21</p></statement>
                    <feedback><p/></feedback>
                </choice>
                <choice>
                    <statement><p>infinite loop</p></statement>
                    <feedback><p/></feedback>
                </choice>
            </choices>
        </exercise>
